#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/jacobcarver/devwork/-INCOMPLETE-/Coding_Caterpillar/node_modules/sshpk/bin/sshpk-sign/node_modules:/Users/jacobcarver/devwork/-INCOMPLETE-/Coding_Caterpillar/node_modules/sshpk/bin/node_modules:/Users/jacobcarver/devwork/-INCOMPLETE-/Coding_Caterpillar/node_modules/sshpk/node_modules:/Users/jacobcarver/devwork/-INCOMPLETE-/Coding_Caterpillar/node_modules:/Users/jacobcarver/devwork/-INCOMPLETE-/node_modules:/Users/jacobcarver/devwork/node_modules:/Users/jacobcarver/node_modules:/Users/node_modules:/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../sshpk/bin/sshpk-sign" "$@"
else 
  exec node  "$basedir/../sshpk/bin/sshpk-sign" "$@"
fi
