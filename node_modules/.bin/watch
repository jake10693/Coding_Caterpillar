#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/jacobcarver/devwork/-INCOMPLETE-/Coding_Caterpillar/node_modules/@cnakazawa/watch/cli.js/node_modules:/Users/jacobcarver/devwork/-INCOMPLETE-/Coding_Caterpillar/node_modules/@cnakazawa/watch/node_modules:/Users/jacobcarver/devwork/-INCOMPLETE-/Coding_Caterpillar/node_modules/@cnakazawa/node_modules:/Users/jacobcarver/devwork/-INCOMPLETE-/Coding_Caterpillar/node_modules:/Users/jacobcarver/devwork/-INCOMPLETE-/node_modules:/Users/jacobcarver/devwork/node_modules:/Users/jacobcarver/node_modules:/Users/node_modules:/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@cnakazawa/watch/cli.js" "$@"
else 
  exec node  "$basedir/../@cnakazawa/watch/cli.js" "$@"
fi
